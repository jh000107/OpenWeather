											"referenceName": "historical_weather_sink",
											"type": "DatasetReference",
											"parameters": {
												"fileName": {
													"value": "@concat('historical_weather_week_', item(), '.json')\n\n",
													"type": "Expression"
												}
											}
										}
									]
								}
							]
						}
					},
